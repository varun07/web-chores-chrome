{
  "name": "InfoEdge Chores",
  "description": "It adds a set of cool buttons and links which eliminates boring and repitive task",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": ["*://*.infoedge.com/*"],      
      "js": ["content_scripts.bundle.js"]
    }
  ],
  "permissions": [
    "activeTab"
  ],
  "icons": {
    "128": "icon-128.png"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
